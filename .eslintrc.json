{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": [0, "always"],
        "no-console": [1],
        "jsx-quotes": [2, "prefer-single"],
        "react/prop-types": ["off"],
        "plugin:react/jsx-runtime": "extends"
    }
}
